<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.fhnw.stefan_kenan.tictactoegui.controller.GameBoard">

    <GridPane xmlns:fx="http://javafx.com/fxml/1">
        <columnConstraints>
            <ColumnConstraints prefWidth="100" />
            <ColumnConstraints prefWidth="100" />
            <ColumnConstraints prefWidth="100" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints prefHeight="100" />
            <RowConstraints prefHeight="100" />
            <RowConstraints prefHeight="100" />
        </rowConstraints>

        <Button fx:id="cell00" prefHeight="100.0" prefWidth="100.0" GridPane.columnIndex="0" GridPane.rowIndex="0" onAction="#handleCell" />
        <Button fx:id="cell01" prefHeight="100.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="0" onAction="#handleCell" />
        <Button fx:id="cell02" prefHeight="100.0" prefWidth="100.0" GridPane.columnIndex="2" GridPane.rowIndex="0" onAction="#handleCell" />
        <Button fx:id="cell10" prefHeight="100.0" prefWidth="100.0" GridPane.columnIndex="0" GridPane.rowIndex="1" onAction="#handleCell" />
        <Button fx:id="cell11" prefHeight="100.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="1" onAction="#handleCell" />
        <Button fx:id="cell12" prefHeight="100.0" prefWidth="100.0" GridPane.columnIndex="2" GridPane.rowIndex="1" onAction="#handleCell" />
        <Button fx:id="cell20" prefHeight="100.0" prefWidth="100.0" GridPane.columnIndex="0" GridPane.rowIndex="2" onAction="#handleCell" />
        <Button fx:id="cell21" prefHeight="100.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="2" onAction="#handleCell" />
        <Button fx:id="cell22" prefHeight="100.0" prefWidth="100.0" GridPane.columnIndex="2" GridPane.rowIndex="2" onAction="#handleCell" />
    </GridPane>


</AnchorPane>
